<div style = "margin-left:50px">
	
<%= form_for :user_list , :url =>  block_users_admin_users_path   do |f| %>
	<% @users.each do |user| %>
		<li>
			<tr>
				<td>
					<%= check_box_tag "user_list[user_ids][]", user.id , user.banned? %>
				</td>

				<td>
					<%= user.email%>
				</td>
			</tr>
		</li>
	<%end%><br/>
	<%= submit_tag "block user"%>
<% end %>
</div>